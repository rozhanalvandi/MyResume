@model Resume.Application.DTOs.SiteSide.HomeIndex.HomeIndexModelDTOs
@{
    ViewData["Title"] = "Home Page";
}


<!-- Content -->
<div class="content" data-simplebar>
    <div class="section mt-0">
        <h1 class="title title--h1 title__separate" style="color: #000000">Resume</h1>
    </div>

    <!-- Education -->
    <div class="section">
        <h2 class="title title--h2"><img class="title-icon" src="/SiteSample/assets/icons/icon-education.svg" alt="" /> Education</h2>
        <div class="timeline">
            <!-- Item -->
            @foreach (var item in Model.Educations)
            {
                <article class="timeline__item">
                    <h5 class="title title--h3 timeline__title" style="color: #4535ac">@item.EducationTitle </h5>
                    <span class="timeline__period" style="color: #5072a7">@item.EducationDuration</span>
                    <p class="timeline__description" style="color: #035096">@item.Description</p>
                </article>

            }



        </div>
    </div>
    <!-- Experience -->
    <div class="section">
        <h2 class="title title--h2"><img class="title-icon" src="/SiteSample/assets/icons/icon-experience.svg" alt="" /> Experience</h2>
        <div class="timeline">
            @foreach (var item in Model.Expriences)
            {
                <article class="timeline__item">
                    <h5 class="title title--h3 timeline__title" style="color: #4535ac">@item.ExprienceTitle</h5>
                    <span class="timeline__period" style="color: #5072a7">@item.ExprienceDuration</span>
                    <p class="timeline__description" style="color: #035096">@item.Description</p>
                </article>
            }

        </div>
    </div>
    <!-- Skills -->
    <div class="section">
        <h2 class="title title--h2" style="color: #4535ac">My Skills</h2>
        <div class="box-gray" style="background-color:white">
            <!-- Progress -->
            @foreach (var item in Model.MySkills)
            {
                <div class="progress">
                    <div class="progress-bar" role="progressbar" aria-valuenow="@item.Perentage" aria-valuemin="0" aria-valuemax="100" style="background-color: #9370db ">
                        <div class="progress-text"><span> @item.SkillTitle</span><span>@item.Perentage</span></div>
                    </div>
                    <div class="progress-text"><span> @item.SkillTitle</span></div>
                </div>

            }

        </div>
    </div>
</div><!-- Content End -->
